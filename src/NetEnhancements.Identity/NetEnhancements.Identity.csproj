    <ProjectReference Include="..\NetEnhancements.Identity.Data\NetEnhancements.Identity.Data.csproj" />
    <ProjectReference Include="..\NetEnhancements.Shared\NetEnhancements.Shared.csproj" />
    <ProjectReference Include="..\NetEnhancements.Util\NetEnhancements.Util.csproj" />
